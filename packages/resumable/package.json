{
  "name": "@xania/resumable",
  "version": "0.1.5",
  "license": "MIT",
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "typings": "./index.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    }
  },
  "private": false,
  "files": [
    "dist",
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "npx vite --watch build",
    "build": "npx vite build",
    "clean": "npx rimraf dist",
    "prepublishOnly": "npm run clean && npm version patch && npm run build"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2
  },
  "author": "Ibrahim ben Salah",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xania/ssr.git"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/prettier": "^2.7.2",
    "vitest": "^0.28.1"
  },
  "peepDependencies": {
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "acorn": "^8.8.2",
    "magic-string": "^0.27.0"
  }
}
